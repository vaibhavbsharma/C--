int a;
int main()
{
	typedef int COUNTERS;
	float someReal12345;
	int a,b,n;
	write("Input some real number:");
	n = read();
	someReal12345 = fread();
	a = 200000;
	while( n != 0) 
	{
		a = a/n;
		n = n - 1;
	}

	/* Using all the logical and arithmatic operator */

	if ( someReal12345 <= 120.05) 
	{
		write("It is less") ;
	} 
	else 
	{
		int a,b,c;
		struct x {
			int a,a1,a2;
			float b;
			int a3[10][10];
		} y1, y2[10];
		if (a == 1) 
		{
		write("A is 1");
		}
		else
		{
			if ( a || n >= 0) 
			{
				write("A greater than zero");
				y1.a3[1][3] = 0;
				y2[3].a3[0][0] = 1;
			} 
			else 
			{
				if (a && n == 0) 
				{
					write("Error in decrementing n");
				}
				else 
				{ 
					if (!(a)) 
					{
						a = 1;
					}
				}
			}
		} 
	}

	return 0; 
}
